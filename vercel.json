{
  "buildCommand": "echo 'No build required'",
  "devCommand": "npm start",
  "framework": "nodejs",
  "headers": [
    {
      "source": "/(.*\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=60, s-maxage=120"
        }
      ]
    }
  ]
}